{
  "_documentation": {
    "purpose": "Defines how often each stream generates events (high/medium/low frequency) with jitter to avoid synchronization. Used by StreamGenerator to schedule event checks.",
    "usage": "Loaded at startup. StreamGenerator uses cadences to schedule event checks: high frequency (1-10 seconds), medium (1-5 minutes), low (30-60 minutes).",
    "example": "customer.tutor.search has medium frequency, so StreamGenerator checks every 1-5 minutes with jitter to determine if a search event should be generated."
  },
  "metadata": {
    "version": "1.0.0",
    "description": "Update frequencies and cadences for all 50 streams in the marketplace simulation",
    "lastUpdated": "2025-01-15"
  },
  "cadenceCategories": {
    "high": {
      "description": "Seconds to minutes - continuous or near-continuous events",
      "checkInterval": {
        "min": 1,
        "max": 10,
        "unit": "seconds"
      },
      "jitter": {
        "min": 0.5,
        "max": 2.0,
        "unit": "seconds"
      }
    },
    "medium": {
      "description": "Minutes to hours - regular but not continuous events",
      "checkInterval": {
        "min": 1,
        "max": 5,
        "unit": "minutes"
      },
      "jitter": {
        "min": 0.5,
        "max": 2.0,
        "unit": "minutes"
      }
    },
    "low": {
      "description": "Hours to daily - periodic or batched events",
      "checkInterval": {
        "min": 30,
        "max": 60,
        "unit": "minutes"
      },
      "jitter": {
        "min": 5,
        "max": 15,
        "unit": "minutes"
      }
    },
    "veryLow": {
      "description": "Occasional or sporadic events",
      "checkInterval": {
        "min": 2,
        "max": 6,
        "unit": "hours"
      },
      "jitter": {
        "min": 30,
        "max": 60,
        "unit": "minutes"
      }
    }
  },
